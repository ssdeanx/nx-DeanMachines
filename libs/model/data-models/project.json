{
  "name": "data-models",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/model/data-models/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/model/data-models",
        "main": "libs/model/data-models/src/index.ts",
        "tsConfig": "libs/model/data-models/tsconfig.lib.json",
        "assets": ["libs/model/data-models/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/model/data-models/**/*.ts"
        ]
      }
    }
  },
  "tags": ["type:model", "scope:shared"]
}
